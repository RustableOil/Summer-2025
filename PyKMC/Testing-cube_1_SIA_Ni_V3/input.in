[Control]
initial_config = ./initconfig-1-sia.xyz
displacements_output = ./displacements.txt
n_steps = 500
engine = lammps
verbosity = 2

[Lammps]
pair_style = eam/alloy
pair_coeff = * * ./FeNiCr_ArturV3.eam Ni
min_style = cg
minimize = 1e-10 1e-12 10000 10000

[AtomicEnvironment]
style = cna/graph
rnei = 3.0
rcut = 6.0

[EventSearch]
style = partn
nsearch = 10
emax_event = 5.0
emin_event = 0.0

[pARTn]
path_artnso = /home/jake/artn-plugin/lib/libartn-lmp.so

[RateConstant]
style = constant
k0 = 1e12
T = 100.0

[PSR]
style = ira

[IRA]
