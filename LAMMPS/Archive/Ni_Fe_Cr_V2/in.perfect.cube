dimension       3

units           metal
boundary        p p p
atom_style      atomic
neighbor        2 bin

lattice         fcc 3.52

variable        S equal ${size}

region          box block 0 ${S} 0 ${S} 0 ${S}
create_box      3 box

mass            1 58.69
mass            2 55.85
mass            3 52.00

pair_style      eam/alloy
pair_coeff      * * FeNiCr_ArturV3.eam Ni Fe Cr

create_atoms    1 box

set             type 1 type/fraction 2 0.10 12345
set             type 1 type/fraction 3 0.18 54321

min_style       cg
minimize        1.0e-6 1.0e-8 10000 100000

dump            2 all custom 1 perfect.custom element type id x y z
dump_modify     2 element Ni Fe Cr
dump_modify     2 format line "%s %d %d %.2f %.2f %.2f"

timestep        0.001

run             1
